+# $MirBSD: obsd.tygs,v 1.18 2003/01/26 13:10:26 tg Exp $
+# Public domain.
+
+PROG=	host
+CFLAGS+=-Wall -Werror
+MLINKS+=host.1 nslookup.1
+
+afterinstall:
+	cd ${.CURDIR} && ${INSTALL} ${INSTALL_COPY} -o root -g bin \
+		-m 555 nslookup ${DESTDIR}${BINDIR}
