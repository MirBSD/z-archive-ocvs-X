index: ports/net/djb-ipv6dns/files/fmt_xlong.c
===================================================================
RCS file: /cvs/ports/net/djb-ipv6dns/files/fmt_xlong.c,v
retrieving revision DEAD
--- /dev/null	Sat Mar 22 14:34:57 2003
+++ ports/net/djb-ipv6dns/files/fmt_xlong.c	Fri Mar  7 18:08:00 2003
@@@@ -0,0 +1,22 @@@@
+#include "fmt.h"
+
+char tohex(char num) {
+  if (num<10)
+    return num+'0';
+  else if (num<16)
+    return num-10+'a';
+  else
+    return -1;
+}
+
+unsigned int fmt_xlong(register char *s,register unsigned long u)
+{
+  register unsigned int len; register unsigned long q;
+  len = 1; q = u;
+  while (q > 15) { ++len; q /= 16; }
+  if (s) {
+    s += len;
+    do { *--s = tohex(u % 16); u /= 16; } while(u); /* handles u == 0 */
+  }
+  return len;
+}
