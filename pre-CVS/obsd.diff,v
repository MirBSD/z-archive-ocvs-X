 int
+Xturnoff()
+{
+	int a1, a2;
+	a1 = a2 = -1;
+	switch(cmd.argc)
+	{
+	case 0:
+		printf("machine off [dev [how] ]\n");
+		return 0;
+	case 1:
+		break;
+	case 3:
+		a2 = strtol(cmd.argv[2], NULL, 0);
+	case 2:
+		a1 = strtol(cmd.argv[1], NULL, 0);
+	}
+	apmturnoff(a1, a2);
+	return 0;
+}
