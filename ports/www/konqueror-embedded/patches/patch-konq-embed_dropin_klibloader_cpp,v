 void *KDLOpenLibrary::symbol( const char *name )
 {
     void *res = dlsym( m_handle, name );
+#ifdef __OpenBSD__
+    if ( !res ) {
+    	char *name2 = (char *)malloc(strlen(name)+2);
+	if (name2) {
+	    name2[0] = '_';
+	    strcpy(name2+1, name);
+	    res = dlsym( m_handle, name2);
+	    free(name2);
+	}
+    }
+#endif
     if ( !res )
         res = KLibrary::symbol( name );
