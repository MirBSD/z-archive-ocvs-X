+    }
     menu Graphics folder {
         prog Gimp gimp gimp
         prog XV xv xv
         prog XPaint xpaint xpaint
         prog XFig xfig xfig
     }
+    menu Multimedia folder {
+        prog XPlayCD xplaycd xplaycd
+        prog XMixer xmixer xmixer
+        prog XMMS xmms xmms
+    }
     menu Devel folder {
         prog ddd ddd ddd
     }
