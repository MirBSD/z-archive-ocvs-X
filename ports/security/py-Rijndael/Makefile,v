PYTHON_VER=	2.1
PY_CMD=		@@cd ${WRKSRC}; python${PYTHON_VER}
PY_SITEPKG=	${PREFIX}/lib/python${PYTHON_VER}/site-packages/
PY_MODNAME=	pyRijndael.py
d24 1
a24 6
SUBST_VARS=	PYTHON_VER

post-extract:
	@@mkdir ${WRKDIST}
	@@${GZIP_CMD} -dc ${FULLDISTDIR}/${DISTNAME}${EXTRACT_SUFX} >	\
		${WRKSRC}/${PY_MODNAME}
d27 1
a27 2
