#!/bin/sh
d4 2
a5 4
# This originally came from X11R5 (mit/util/scripts/install.sh).

scriptversion=2003-01-17.15

a6 1
# (FSF changes in the public domain.)
d26 1
d32 1
d44 2
a45 2
transformbasename=
transform_arg=
d48 3
a50 3
