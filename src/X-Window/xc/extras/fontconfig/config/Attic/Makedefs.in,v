SHELL = @@SHELL@@

srcdir=@@srcdir@@
prefix=@@prefix@@
exec_prefix=@@exec_prefix@@
libdir=@@libdir@@
bindir=@@bindir@@
datadir=@@datadir@@
includedir=@@includedir@@
sysconfdir=@@sysconfdir@@
confdir=@@confdir@@

DSO_LDOPTS=@@DSO_LDOPTS@@
