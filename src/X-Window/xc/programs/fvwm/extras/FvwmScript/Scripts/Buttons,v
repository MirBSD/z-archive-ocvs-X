   If (GetValue 9)==3 Then
    Do {Exec xedit &}
   If (GetValue 9)==4 Then
    Do {Exec coaxe &}
   If (GetValue 9)==5 Then
    Do {Exec bash -c "netscape" &}
   If (GetValue 9)==6 Then
    Do {Exec xclipboard &}
   If (GetValue 9)==7 Then
    Do {Exec plan &}
  End
End

Widget 10
Property
 Size 1 1
 Position 471 10
 Type PushButton
 Title {|Random|Bat|Blot|Bouboule|Bounce|Flame|Galaxy|Grav|Helix|Hop|Maze}
