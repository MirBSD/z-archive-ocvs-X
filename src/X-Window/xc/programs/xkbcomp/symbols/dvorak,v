// par Josselin Mouette <josselin.mouette@@ens-lyon.org>.

partial alphanumeric_keys modifier_keys 
xkb_symbols "fr" {

    include "dvorak(basic)"

    name[Group1]= "French dvorak";

    key <TLDE>  {       [              oe,              OE      ],
