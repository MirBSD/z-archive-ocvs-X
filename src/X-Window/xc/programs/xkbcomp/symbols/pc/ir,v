// $XFree86: xc/programs/xkbcomp/symbols/pc/ir,v 1.2 2002/11/22 04:03:28 dawes Exp $

partial default alphanumeric_keys 
xkb_symbols "basic" {
      name[Group1]= "Iranian";

      key <TLDE> { [	0x100200d	]	};

      // Mainly numbers.
