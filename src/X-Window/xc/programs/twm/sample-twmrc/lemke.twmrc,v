TitleButtonBorderWidth	1
UnknownIcon     "ncdlogo64.xbm"
#WarpCursor
XORValue	1
#Zoom

DefaultFunction	f.nop

Function "move-or-raise"
{
	f.move
	f.deltastop
	f.raise
}

