"Xterm"			!"/usr/bin/X11/xterm -ls -display $DISPLAY &"
"Big Xterm"		!"/usr/bin/X11/xterm -ls -fn 9x15 -display $DISPLAY &"
"xsol"			!"$HOME/games/bin/`arch`/xsol &"
"Spider"		!"$HOME/games/bin/`arch`/spider &"
"Utility"		f.title
#"Focus"		f.focus
