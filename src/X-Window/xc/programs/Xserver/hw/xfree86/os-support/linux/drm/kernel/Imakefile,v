InstallMultiple($(SRCS),$(DRMSRCDIR))
install::
	MakeDir($(DESTDIR)$(DRMSRCDIR))
	@@(cd $(DESTDIR)$(DRMSRCDIR); rm -f Makefile; set -x; ln Makefile.linux Makefile)

