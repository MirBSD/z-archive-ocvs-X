       (((pNv->Chipset & 0x0ff0) != 0x0110) ||
        !(pNv->riva.flatPanel & FP_DITHER)))
    {
       infoPtr->UseHWCursorARGB = NVUseHWCursorARGB;
       infoPtr->LoadCursorARGB = NVLoadCursorARGB;
    }
#endif
