#include "atioption.h"
#include "atiprobe.h"
#include "ativersion.h"

/* The root of all evil... */
DriverRec ATI =
{
    ATI_VERSION_CURRENT,
    "ati",
    ATIIdentify,
    ATIProbe,
    ATIAvailableOptions,
    NULL,
    0
};
