typedef struct _SiS300_Ext2Struct
{
	USHORT Ext_InfoFlag;
	UCHAR Ext_CRT1CRTC;  /* TW: Index in SiS300_CRT1Table */
	UCHAR Ext_CRTVCLK;   /* TW: Index in VCLK array */
	UCHAR Ext_CRT2CRTC;  /* TW: Index in LCD Paneltype arrays (&3f) */
	UCHAR  ModeID;
	USHORT XRes;
	USHORT YRes;
	USHORT ROM_OFFSET;
} SiS300_Ext2Struct;

