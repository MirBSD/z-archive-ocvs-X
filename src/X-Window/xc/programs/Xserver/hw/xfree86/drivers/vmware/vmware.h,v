@d6 1
a6 1
/* $XFree86: xc/programs/Xserver/hw/xfree86/drivers/vmware/vmware.h,v 1.12 2003/11/17 22:20:38 dawes Exp $ */
d16 1
a16 1
#include "compiler.h"	        /* inb/outb */
