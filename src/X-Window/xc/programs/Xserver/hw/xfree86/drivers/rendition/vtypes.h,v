a100 17
typedef struct _renditionRegs {
    CARD8 mode;
    CARD8 memendian;
    CARD32 sclkpll;
    CARD32 dramctl;
    CARD8 plldev;
    CARD32 pclkpll;
    CARD8 daccmd0;
    CARD8 daccmd1;
    CARD8 daccmd2;
    CARD8 daccmd3;
    CARD32 crtch;
    CARD32 crtcv;
    CARD32 vbasea;
    CARD32 crtcoff;
    CARD32 crtcctl;
} RenditionRegRec, *RenditionRegPtr;
