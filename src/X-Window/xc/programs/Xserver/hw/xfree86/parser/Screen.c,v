a594 12
XF86ConfDisplayPtr
xf86findDisplay (int depth, XF86ConfDisplayPtr p)
{
	while (p)
	{
		if (depth == p->disp_depth)
			return (p);

		p = p->list.next;
	}
	return (NULL);
}
