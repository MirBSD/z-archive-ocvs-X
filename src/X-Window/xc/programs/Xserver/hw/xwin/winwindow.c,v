  ErrorF ("winMapWindowPRootless ()\n");
#endif

  fResult = winGetScreenPriv(pWin->drawable.pScreen)->RealizeWindow(pWin);
  
  winReshapePRootless (pWin);
  
  winUpdateRgnPRootless (pWin);
  
  return fResult;
}


