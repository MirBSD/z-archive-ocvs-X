void xnestPolyArc(pDrawable, pGC, nArcs, pArcs)
     DrawablePtr pDrawable;
     GCPtr pGC;
     int nArcs;
     XArc *pArcs;
{
  XDrawArcs(xnestDisplay, xnestDrawable(pDrawable), xnestGC(pGC),
            pArcs, nArcs);
}

void xnestFillPolygon(pDrawable, pGC, shape, mode, nPoints, pPoints)
     DrawablePtr pDrawable;
     GCPtr pGC;
     int shape;
     int mode;
     int nPoints;
     XPoint *pPoints;
