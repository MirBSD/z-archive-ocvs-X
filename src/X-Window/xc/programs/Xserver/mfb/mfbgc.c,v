a745 39
/* some noop functions */
static void
mfbPolyGlyphBltNoop(
    DrawablePtr pDrawable,
    GCPtr pGC,
    int x,
    int y,
    unsigned int nglyph,
    CharInfoPtr * ppci,
    pointer pglyphBase)
{
    /* this is a no-op function */
}

static void
mfbNoopFS(
    DrawablePtr pDrawable,
    GCPtr pGC,
    int nInit,
    DDXPointPtr pptInit,
    int * pwidthInit,
    int fSorted)
{
    /* this is a no-op function */
}

