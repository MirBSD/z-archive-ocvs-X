#ifdef __OpenBSD__
#include <sys/param.h>
/* 2.8 (200012) doesn't have _PW_NAME_LEN */
#if OpenBSD > 200012
#define HAVE_PW_NAME_LEN
#endif
#endif

#ifndef HAVE_PW_NAME_LEN
