@d4 1
a4 1
/* $XFree86: xc/programs/xgc/gram.y,v 1.4 2003/05/07 21:02:07 herrb Exp $ */
d11 1
a11 2
#include <X11/Intrinsic.h>
#include "xgc.h"
d14 20
a33 1

d112 2
a113 1
yyerror(const char *s)
@


