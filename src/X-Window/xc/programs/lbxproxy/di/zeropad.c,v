ZeroGrabKeyboardReq (req)
    xGrabKeyboardReq *req;
{
    req->pad = 0;
}

static void
ZeroGrabKeyReq (req)
    xGrabKeyReq *req;
{
    req->pad1 = 0;
    req->pad2 = 0;
    req->pad3  = 0;
}

