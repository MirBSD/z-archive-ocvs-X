		            VF_TEXCOORD_COUNT_1 |	\
		            VF_SPEC_FOG_ENABLE |	\
		            VF_RGBA_ENABLE |		\
		            VF_XYZW)

#define TEX1_VERTEX_FORMAT (GFX_OP_VERTEX_FMT |		\
		            VF_TEXCOORD_COUNT_2 |	\
		            VF_SPEC_FOG_ENABLE |	\
		            VF_RGBA_ENABLE |		\
		            VF_XYZW)

#define PROJ_TEX1_VERTEX_FORMAT 0
#define TEX2_VERTEX_FORMAT      0
#define TEX3_VERTEX_FORMAT      0
#define PROJ_TEX3_VERTEX_FORMAT 0

