      DRMOBJS = $(GLXLIBSRC)/dri/drm/?*.o
     DRMUOBJS = $(GLXLIBSRC)/dri/drm/unshared/?*.o
     DRMDOBJS = $(GLXLIBSRC)/dri/drm/debugger/?*.o
     DRMPOBJS = $(GLXLIBSRC)/dri/drm/profiled/?*.o

    MESAOBJS = $(COREMESAOBJS)  $(MESA_ASM_OBJS)
